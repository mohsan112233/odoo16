<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_advance_salary" model="ir.sequence">
            <field name="name">Advance Salary Sequence</field>
            <field name="code">advance_salary</field>
            <field name="prefix">ADV/SAL/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">4</field>
        </record>

        <record id="sequence_loan_salary" model="ir.sequence">
            <field name="name">Loan Sequence</field>
            <field name="code">loan_salary</field>
            <field name="prefix">LOAN/SAL/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>